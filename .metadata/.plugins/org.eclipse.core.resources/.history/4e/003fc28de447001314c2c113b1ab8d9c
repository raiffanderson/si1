
public class ConverteNumeros {

	static String[] numeros = {"zero","um", "dois", "tres","quatro","cinco","seis","sete","oito","nove","dez","onze","doze","treze","quatorze","quinze","dezesseis","dezessete","dezoito","dezenove"};

	
	
	public static String devolveNumeroPorExtenso(int num) {
		/*1ºcaso - numeros de 0 a 10*/
		return numeros[num];
	}
	
	

}
